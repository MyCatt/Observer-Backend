{
    "layoutPaths": {
        "Section": "//div[@data-dyn-role='Section']//div[@role='heading']//button[text()]//ancestor::div[@data-dyn-role='SectionPage']",
        "Grid": "//div[@role='region'][descendant-or-self::h1/span[text()]]//div[@role='grid'][not(.//ancestor::*[contains(@style, 'display: none')])]",
        "SideForm": "//div[@data-dyn-role='Dialog']//div[@role='heading']/ancestor::div[@data-dyn-role='Dialog']//div[@data-dyn-controlname='MainGroup']",
        "Dialog": "//div[@data-dyn-role='Dialog']",
        "SysBoxForm": "//div[@data-dyn-form-name='SysBoxForm']",
        "ActionPane": "//div[@data-dyn-role='AppBarSection']",
        "ActionPaneShadow": "//div[not(@data-dyn-role='ContextMenu') and contains(@class, 'sysPopup') and @role='menu']",
        "Popup": "//form[@data-dyn-role='Popup']",
        "Listbox": "//ul[@role='listbox']"
    },
    "textPath" : {
        "Section": "//div[@role='heading']//button[text()]",
