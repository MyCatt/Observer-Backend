        "Dialog": "//div[contains(@class, 'dialog-header')]//div[@role='heading']",
        "ActionPane": "exact:Generic",
        "ActionPaneShadow": "exact:Shadow",
        "Popup": "exact:Lookup Form",
        "SysBoxForm": "exact:SysBoxForm",
        "Listbox": "exact:Listbox"
    },
    "readableLayouts" : {
        "Section": "Generic - LAYOUT.name Section",
        "Grid": "Generic - LAYOUT.name Grid",
        "SideForm": "Generic - LAYOUT.name Form",
        "Dialog": "Generic - LAYOUT.name Form",
